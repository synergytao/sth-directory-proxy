{
  "routes": [
    {
      "src": "/api/save",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-API-Key",
        "Access-Control-Max-Age": "86400"
      },
      "status": 204
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
